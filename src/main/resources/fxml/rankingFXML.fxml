<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" maxHeight="100.0" maxWidth="800.0" prefHeight="100.0" prefWidth="800.0" spacing="100.0" styleClass="rankingContainer" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.myshelfie.view.GUI.RankingControllerFX">
   <children>
      <Label fx:id="username_LBL" stylesheets="@../css/style.css" text="Usernameplayer">
         <styleClass>
            <String fx:value="usernameLBL" />
            <String fx:value="textEndGame" />
            <String fx:value="h2" />
         </styleClass></Label>
      <Label fx:id="points_LB" stylesheets="@../css/style.css" text="Points">
         <styleClass>
            <String fx:value="textEndGame" />
            <String fx:value="h2" />
            <String fx:value="pointsLBL" />
         </styleClass></Label>
      <ImageView fx:id="winner_IMG" disable="true" fitHeight="75.0" fitWidth="75.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../graphics/tokens/endGame.jpg" />
         </image>
      </ImageView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <opaqueInsets>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </opaqueInsets>
</HBox>
