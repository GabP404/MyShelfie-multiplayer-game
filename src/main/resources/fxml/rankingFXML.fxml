   <children>
      <Label fx:id="username_LBL" text="Usernameplayer" />
      <Label fx:id="points_LB" text="Points" />
      <ImageView fx:id="winner_IMG" disable="true" fitHeight="50.0" fitWidth="50.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../graphics/winner/crown_winner.png" />
         </image>
      </ImageView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" />
   </padding>
</HBox>
